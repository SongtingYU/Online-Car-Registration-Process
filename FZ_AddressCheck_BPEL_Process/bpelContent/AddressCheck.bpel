<!-- AddressCheck BPEL Process [Generated by the Eclipse BPEL Designer] -->
<!-- Date: Wed Sep 28 14:16:23 AEST 2016 -->
<bpel:process name="AddressCheck"
	targetNamespace="http://gnafaddressservice.soacourse.unsw.edu.au/addresscheck"
	suppressJoinFailure="yes"
	xmlns:tns="http://gnafaddressservice.soacourse.unsw.edu.au/addresscheck"
	xmlns:addrc="http://gnafaddressservice.soacourse.unsw.edu.au"
	xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" exitOnStandardFault="yes">

	<!-- Import the client WSDL -->
	<bpel:import location="AddressCheckArtifacts.wsdl"
		namespace="http://gnafaddressservice.soacourse.unsw.edu.au/addresscheck"
		importType="http://schemas.xmlsoap.org/wsdl/" />
	<bpel:import namespace="http://gnafaddressservice.soacourse.unsw.edu.au"
		importType="http://schemas.xmlsoap.org/wsdl/" location="GNAFAddressService.wsdl" />

	<!-- ================================================================= -->
	<!-- PARTNERLINKS -->
	<!-- List of services participating in this BPEL process -->
	<!-- ================================================================= -->
	<bpel:partnerLinks>
		<!-- The 'client' role represents the requester of this service. -->
		<bpel:partnerLink name="client" partnerLinkType="tns:AddressCheck"
			myRole="AddressCheckProvider" />
		<bpel:partnerLink name="addrcRes" partnerLinkType="tns:GNAFLinkType"
			partnerRole="GNAFAddressServiceProvider" />
	</bpel:partnerLinks>

	<!-- ================================================================= -->
	<!-- VARIABLES -->
	<!-- List of messages and XML documents used within this BPEL process -->
	<!-- ================================================================= -->
	<bpel:messageExchanges>
		<bpel:messageExchange name="messageExchange"></bpel:messageExchange>
	</bpel:messageExchanges>
	<bpel:variables>
		<!-- Reference to the message passed as input during initiation -->
		<bpel:variable name="input" messageType="tns:AddressCheckRequestMessage" />
		<bpel:variable name="ExistsRequest" messageType="addrc:addressExistsInFile" />
		<bpel:variable name="SimilarRequest" messageType="addrc:similarAddressesInFile" />

		<!-- Reference to the message that will be returned to the requester -->
		<bpel:variable name="ExistsResponse" messageType="addrc:addressExistsInFileResponse" />
		<bpel:variable name="ExistsFault" messageType="addrc:AddressExistsInFileFaultMsg" />

		<bpel:variable name="SimilarResponse"
			messageType="addrc:similarAddressesInFileResponse" />
		<bpel:variable name="SimilarFault"
			messageType="addrc:SimilarAddressesInFileFaultMsg" />


		<bpel:variable name="output" messageType="tns:AddressCheckResponseMessage" />
	</bpel:variables>

	<!-- ================================================================= -->
	<!-- ORCHESTRATION LOGIC -->
	<!-- Set of activities coordinating the flow of messages across the -->
	<!-- services integrated within this business process -->
	<!-- ================================================================= -->

	<bpel:sequence name="main">

		<!-- Receive input from requester. Note: This maps to operation defined 
			in AddressCheck.wsdl -->

		<bpel:receive name="receiveInput" partnerLink="client"
			portType="tns:AddressCheck" operation="process" variable="input"
			createInstance="yes" />

		<!-- Generate reply to synchronous request -->


		<bpel:assign validate="no" name="InitOutput">
			<bpel:copy>
				<bpel:from>
					<bpel:literal xml:space="preserve"><tns:AddressCheckResponse
						xmlns:tns="http://gnafaddressservice.soacourse.unsw.edu.au/addresscheck"
						xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:CheckResponseCode></tns:CheckResponseCode>
  <tns:CheckResponseData></tns:CheckResponseData>
  <tns:CheckResponseStatus>Output closed</tns:CheckResponseStatus>
</tns:AddressCheckResponse></bpel:literal>
				</bpel:from>
				<bpel:to variable="output" part="payload"></bpel:to>
			</bpel:copy>
            
        </bpel:assign>
		<bpel:sequence name="SeqExactAddr">

			<bpel:assign validate="no" name="TransInputToReq">
				<bpel:copy>
					<bpel:from>
						<bpel:literal>
							<ns1:addressExistsInFileRequest
								xmlns:ns1="http://gnafaddressservice.soacourse.unsw.edu.au"
								xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
								<address>address</address>
							</ns1:addressExistsInFileRequest>
						</bpel:literal>
					</bpel:from>
					<bpel:to variable="ExistsRequest" part="parameters"></bpel:to>
				</bpel:copy>
				<bpel:copy>
					<bpel:from part="payload" variable="input">
						<bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:address]]></bpel:query>
					</bpel:from>
					<bpel:to part="parameters" variable="ExistsRequest">
						<bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[address]]></bpel:query>
					</bpel:to>
				</bpel:copy>
			</bpel:assign>

			<bpel:invoke name="AddrExactCheck" partnerLink="addrcRes"
				operation="addressExistsInFile" portType="addrc:GNAFAddressService"
				inputVariable="ExistsRequest" outputVariable="ExistsResponse">

				<bpel:catch faultMessageType="addrc:AddressExistsInFileFaultMsg"
					faultVariable="ExistsFault">

					<bpel:sequence>


						<bpel:assign validate="no" name="TransFaultToString">
                            
                        
                            
                        
                            
                            
                            <bpel:copy>
                                <bpel:from part="AddressExistsInFileFaultMsg" variable="ExistsFault">
                                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[errtext]]></bpel:query>
                                </bpel:from>
                                <bpel:to part="payload" variable="output">
                                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:CheckResponseData]]></bpel:query>
                                </bpel:to>
                            </bpel:copy>
                            
                            <bpel:copy>
                                <bpel:from>
                                    <bpel:literal xml:space="preserve">Fault</bpel:literal>
                                </bpel:from>
                                <bpel:to part="payload" variable="output">
                                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:CheckResponseCode]]></bpel:query>
                                </bpel:to>
                            </bpel:copy>
                        </bpel:assign>
                        <bpel:reply name="ReplyOutput" partnerLink="client" operation="process" portType="tns:AddressCheck" variable="output"></bpel:reply>
                        <bpel:exit name="Exit"></bpel:exit>
                    </bpel:sequence>
                </bpel:catch>
               </bpel:invoke>



		</bpel:sequence>


		<bpel:if name="IfAddrExact">
			<bpel:condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[$ExistsResponse.parameters//returnData != 'NOT FOUND']]></bpel:condition>
			<bpel:sequence name="AddrExact">
				<bpel:assign validate="no" name="ExactFinishOutput">
					
                    
                    
                    
                    
                    <bpel:copy>
                        <bpel:from>
                            <bpel:literal xml:space="preserve">Exact Address Found</bpel:literal>
                        </bpel:from>
                        <bpel:to part="payload" variable="output">
                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:CheckResponseCode]]></bpel:query>
                        </bpel:to>
                    </bpel:copy>
                </bpel:assign>
			</bpel:sequence>
			<bpel:else>
				<bpel:sequence name="ANotExact">
					<bpel:assign validate="no" name="TransInputToSimReq">
						<bpel:copy>
							<bpel:from>
								<bpel:literal>
									<ns1:similarAddressesInFileRequest
										xmlns:ns1="http://gnafaddressservice.soacourse.unsw.edu.au"
										xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
										<address>address</address>
									</ns1:similarAddressesInFileRequest>
								</bpel:literal>
							</bpel:from>
							<bpel:to variable="SimilarRequest" part="parameters"></bpel:to>
						</bpel:copy>
						<bpel:copy>
							<bpel:from part="payload" variable="input">
								<bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:address]]></bpel:query>
							</bpel:from>
							<bpel:to part="parameters" variable="SimilarRequest">
								<bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[address]]></bpel:query>
							</bpel:to>
						</bpel:copy>
					</bpel:assign>
					<bpel:invoke name="AddrSimilarCheck" partnerLink="addrcRes"
						operation="similarAddressesInFile" portType="addrc:GNAFAddressService"
						inputVariable="SimilarRequest" outputVariable="SimilarResponse"></bpel:invoke>
					
					<bpel:if name="IfSimilarExist">
						<bpel:condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[$SimilarResponse.parameters//returnData != '']]></bpel:condition>
						<bpel:assign validate="no" name="SimilarFinishOutput">
							
                            <bpel:copy>
                                <bpel:from><bpel:literal xml:space="preserve">Exact Address Not Found.
Similar Addresses Found, List included below</bpel:literal></bpel:from>
                                <bpel:to part="payload" variable="output">
                                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:CheckResponseCode]]></bpel:query>
                                </bpel:to>
                            </bpel:copy>
                            
                            
                            
                            <bpel:copy><bpel:from part="parameters" variable="SimilarResponse">
                                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                                        <![CDATA[returnData]]>
                                    </bpel:query>
                                </bpel:from>
                                <bpel:to part="payload" variable="output">
                                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                                        <![CDATA[tns:CheckResponseData]]>
                                    </bpel:query>
                                </bpel:to>
                            
                            </bpel:copy>
                            
                        </bpel:assign>
						<bpel:else>
							<bpel:assign validate="no" name="NoSimilarOutput">
								
                                
                                
                                <bpel:copy>
                                    <bpel:from>
                                        <bpel:literal xml:space="preserve">Exact Address Not Found</bpel:literal>
                                    </bpel:from>
                                    <bpel:to part="payload" variable="output">
                                        <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:CheckResponseCode]]></bpel:query>
                                    </bpel:to>
                                </bpel:copy>
                                <bpel:copy>
                                    <bpel:from>
                                        <bpel:literal xml:space="preserve">Similar Addresses Not</bpel:literal>
                                    </bpel:from>
                                    <bpel:to part="payload" variable="output">
                                        <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:CheckResponseData]]></bpel:query>
                                    </bpel:to>
                                </bpel:copy>
                            </bpel:assign>
						</bpel:else>
					</bpel:if>
				</bpel:sequence>
			</bpel:else>
		</bpel:if>
		<bpel:reply name="replyOutput" partnerLink="client"
			portType="tns:AddressCheck" operation="process" variable="output" />

	</bpel:sequence>
</bpel:process>

